<term_definitions code="at0000">
    <items id="text">Substance use summary</items>
    <items id="description">Summary or persistent information about the typical use of a single addictive, or potentially addictive, substance by an individual.</items>
</term_definitions>
<term_definitions code="at0001">
    <items id="text">Item tree</items>
    <items id="description">@ internal @</items>
</term_definitions>
<term_definitions code="at0002">
    <items id="text">Substance name</items>
    <items id="description">The name of the substance or substance group/class used.</items>
    <items id="comment">Coding with an external terminology is preferred, where possible.</items>
</term_definitions>
<term_definitions code="at0003">
    <items id="text">Overall status</items>
    <items id="description">Statement about current use of the substance, in all forms and by all routes.</items>
</term_definitions>
<term_definitions code="at0004">
    <items id="text">Never used</items>
    <items id="description">The individual has never used the identified substance.</items>
</term_definitions>
<term_definitions code="at0005">
    <items id="text">Current user</items>
    <items id="description">The individual is currently using the identified substance.</items>
</term_definitions>
<term_definitions code="at0006">
    <items id="text">Former user</items>
    <items id="description">The individual has previously used the identified substance.</items>
</term_definitions>
<term_definitions code="at0007">
    <items id="text">Overall description</items>
    <items id="description">Narrative summary about use behaviour for the substance.</items>
</term_definitions>
<term_definitions code="at0008">
    <items id="text">First ever use</items>
    <items id="description">Date when the individual first used the substance.</items>
    <items id="comment">Can be a partial date, for example, only a year.</items>
</term_definitions>
<term_definitions code="at0009">
    <items id="text">Regular use commenced</items>
    <items id="description">The date when the individual first started frequent or regular, but usually non-daily, use of the substance.</items>
    <items id="comment">Can be a partial date, for example, only a year. For example, this date could represent when the individual commenced using the substance every Friday night or at parties.</items>
</term_definitions>
<term_definitions code="at0010">
    <items id="text">Daily use commenced</items>
    <items id="description">The date when the individual first started daily use of the substance.</items>
    <items id="comment">Can be a partial date, for example, only a year.</items>
</term_definitions>
<term_definitions code="at0011">
    <items id="text">Typical use</items>
    <items id="description">The typical amount and frequency of use of the substance, if consistent over time.</items>
    <items id="comment">This data element effectively becomes redundant if usage patterns are recorded using the 'Per episode' cluster. If it is used concurrently with the 'Per episode' data elements, it is possible for the data recorded using this data element to become inconsistent or outdated as more detailed data is added to the 'Per episode' cluster.</items>
</term_definitions>
<term_definitions code="at0012">
    <items id="text">Per episode</items>
    <items id="description">Details about a discrete period of use of the substance.</items>
</term_definitions>
<term_definitions code="at0016">
    <items id="text">Specific substance</items>
    <items id="description">The name of the specific substance used.</items>
    <items id="comment">This data element may be redundant if the specific substance has been identified in the 'Substance name' data element. However if The 'Substance name' data element is recorded as a grouping or a class, it may be necessary to provide more specificity for a specific episode.</items>
</term_definitions>
<term_definitions code="at0017">
    <items id="text">Status</items>
    <items id="description">Date when this episode commenced.</items>
    <items id="comment">Can be a partial date, for example, only a year.</items>
</term_definitions>
<term_definitions code="at0013">
    <items id="text">Episode label</items>
    <items id="description">Identification of an episode of substance use - either as a number in a sequence or a named event.</items>
    <items id="comment">For example: '2' as the second episode within a sequence of episodes; or 'Pregnancy with twins' if describing the substance use during a health event such as a specific pregnancy.</items>
</term_definitions>
<term_definitions code="at0014">
    <items id="text">Episode start date</items>
    <items id="description">Date when this episode commenced.</items>
    <items id="comment">Can be a partial date, for example, only a year.</items>
</term_definitions>
<term_definitions code="at0018">
    <items id="text">Episode Description</items>
    <items id="description">Narrative summary about substance use during the episode.</items>
</term_definitions>
<term_definitions code="at0015">
    <items id="text">Episode end date</items>
    <items id="description">Date when this episode ceased.</items>
    <items id="comment">Can be a partial date, for example, only a year. This data field will be empty if the episode is current and ongoing.</items>
</term_definitions>
<term_definitions code="at0019">
    <items id="text">Pattern</items>
    <items id="description">The typical pattern of substance use.</items>
    <items id="comment">The typical pattern of use can be made more granular by coding with a terminology or a local value set in a template.</items>
</term_definitions>
<term_definitions code="at0020">
    <items id="text">Daily</items>
    <items id="description">Use the substance at least once every day.</items>
</term_definitions>
<term_definitions code="at0021">
    <items id="text">Non-daily</items>
    <items id="description">Not using the substance every day.</items>
</term_definitions>
<term_definitions code="at0022">
    <items id="text">Substance free days</items>
    <items id="description">The number of days where the substance was not used.</items>
</term_definitions>
<term_definitions code="at0023">
    <items id="text">Typical use</items>
    <items id="description">Typical amount and frequency of use.</items>
</term_definitions>
<term_definitions code="at0024">
    <items id="text">Number of quit attempts</items>
    <items id="description">Total number of times the individual has attempted to stop using the specified substance within this episode.</items>
</term_definitions>
<term_definitions code="at0025">
    <items id="text">Episode details</items>
    <items id="description">Additional details about the episode.</items>
</term_definitions>
<term_definitions code="at0026">
    <items id="text">Episode comment</items>
    <items id="description">Additional narrative about use of the specific substance during this episode, not captured in other fields.</items>
</term_definitions>
<term_definitions code="at0027">
    <items id="text">Overall details</items>
    <items id="description">Additional structured details about the overall use of the specified substance.</items>
</term_definitions>
<term_definitions code="at0028">
    <items id="text">Overall quit date</items>
    <items id="description">The date when the individual last ceased all use of the substance.</items>
</term_definitions>
<term_definitions code="at0029">
    <items id="text">Item tree</items>
    <items id="description">@ internal @</items>
</term_definitions>
<term_definitions code="at0030">
    <items id="text">Overall comment</items>
    <items id="description">Additional narrative about the overall use of the substance that has not been captured in other fields.</items>
</term_definitions>
<term_definitions code="at0031">
    <items id="text">Last updated</items>
    <items id="description">The date this summary was last updated.</items>
</term_definitions>
<term_definitions code="at0032">
    <items id="text">Extension</items>
    <items id="description">Additional information required to extend the model with local content or to align with other reference models/formalisms.</items>
    <items id="comment">For example: local information requirements; or additional metadata to align with FHIR.</items>
</term_definitions>
<term_definitions code="at0033">
    <items id="text">Per form/route</items>
    <items id="description">Details about the substance use per form of the substance or per route of administration, during the episode.</items>
</term_definitions>
<term_definitions code="at0034">
    <items id="text">Per form</items>
    <items id="description">Details about each form of the substance. For example: buds or oil.</items>
</term_definitions>
<term_definitions code="at0035">
    <items id="text">Per route</items>
    <items id="description">Details about each route of administration . For example: intravenous or inhalation.</items>
</term_definitions>
<term_definitions code="at0036">
    <items id="text">Form/route</items>
    <items id="description">The name of the form or route.</items>
</term_definitions>
<term_definitions code="at0037">
    <items id="text">Description</items>
    <items id="description">Narrative description about the substance use by the identified form or route.</items>
</term_definitions>
<term_definitions code="at0038">
    <items id="text">Form</items>
    <items id="description">The form of the substance administered.</items>
</term_definitions>
<term_definitions code="at0039">
    <items id="text">Route</items>
    <items id="description">The route of administration of the substance.</items>
</term_definitions>
<term_definitions code="at0040">
    <items id="text">Typical use</items>
    <items id="description">Typical use by the identified form or route.</items>
</term_definitions>
<term_definitions code="at0041">
    <items id="text">Comment</items>
    <items id="description">Additional narrative about the substance use by the identified form or route that has not been captured in other fields.</items>
</term_definitions>
